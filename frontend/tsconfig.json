{
  "include": [
    "frontend/src/**/*"
  ],

  "compilerOptions": {
    "esModuleInterop": true,
    "lib": [
      "dom",
      "es2015"
    ],
    "jsx": "react-jsx",
    "target": "es2015",
    "allowJs": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "strict": true,
    "noEmitOnError": false,
    "noImplicitAny": true,
    "typeRoots": [
      "./node_modules/@types/"
    ]
  }
}
